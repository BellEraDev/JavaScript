### ArrayJS.html
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>    
</head>
<body>
    <script src='ArrayJS.js'></script>
</body>
</html>

### ArrayJS.js
let scores = [82, 75, 48, 64, 36];
let passFailScores = scores.map((score) => {
    if (score >= 50) {
        return 'passed';
    }
    return 'failed';
});

console.log(scores);
console.log(passFailScores);

### ArrayJS1.html
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>    
</head>
<body>
    <script src='ArrayJS1.js'></script>
</body>
</html>

### ArrayJS1.js
let scores = [82, 75, 48, 64, 36];
let someonePassed = scores.some((score) => {
    return score >= 50;
});

console.log(someonePassed);
